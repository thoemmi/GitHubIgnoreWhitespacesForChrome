{
  "name": "Ignore whitespaces on GitHub",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Forces diff views on GitHub to ignore whitespaces.",
  "icons": { 
	"16": "icon16x16.png", 
	"48": "icon48x48.png", 
	"128": "icon128x128.png" },
  "permissions": [
	"webRequest", 
	"webRequestBlocking", 
	"https://github.com/"],
  "background": { 
	"scripts": [ "jsuri-1.1.1.min.js", "background.js" ]
  }
}