{
  "name": "Ignore whitespaces on GitHub",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Forces diff views on GitHub to ignore whitespaces.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
	"webRequest", 
	"webRequestBlocking", 
	"background", 
	"https://github.com/"],
  "background": { 
	"scripts": [ "jsuri-1.1.1.min.js", "background.js" ]
  }
}